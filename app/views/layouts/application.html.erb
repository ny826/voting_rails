<!DOCTYPE html>
<html>
<head>
  <title>Voting</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user%>

<header>
	<h1>Online Voting</h1>
	<%= link_to 'logout', destroy_user_session_path, method: :delete %>
</header>

<%end%>

<%= yield %>

</body>
</html>
